---
# metadata for table ajax
---
{% assign metadata = site.metadata | split: ';' %}
{% for m in metadata %}
{% assign items = items | concat: site.data[m] %}
{% endfor %}
{% assign items = items | where_exp: 'item','item.objectid' %}
{%- assign fields = site.data.config-table | map: 'field' -%}
{ "objects": [
{%- for item in items -%}[ {% for f in fields %}{% if item[f] %}{{ item[f] | jsonify }}{% else %}""{% endif %},{% endfor %} {{ "/items/" | append: item.objectid | append: ".html" | relative_url | jsonify }} ]{% unless forloop.last %},{% endunless %}
{% endfor %}
] }